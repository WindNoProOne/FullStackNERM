<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Get Crud View</title>
    <style>
        table {
            border-collapse: collapse;
            width: 100%;
            font-size: 13px;
        }

        td,
        th {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }

        tr:nth-child(even) {
            background-color: #dddddd;
        }
    </style>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.4/dist/jquery.slim.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
</head>

<body>
    <table>
        <tr>
            <th>Email</th>
            <th>Password</th>
            <th>First Name</th>
            <th>Lats Name</th>
            <th>Address</th>
            <th>Gender</th>
            <th>RoledId</th>
            <th>Phone Number</th>
        </tr>
        <% for(let i=0; i < dataTable.length; i++) { %>
            <tr>
                <th>
                    <%= dataTable[i].email %>
                </th>
                <th>
                    <%= dataTable[i].password %>
                </th>
                <th>
                    <%= dataTable[i].firstName %>
                </th>
                <th>
                    <%= dataTable[i].lastName %>
                </th>
                <th>
                    <%= dataTable[i].address %>
                </th>
                <th>
                    <%= dataTable[i].gender %>
                </th>
                <th>
                    <%= dataTable[i].roleId %>
                </th>
                <th>
                    <%= dataTable[i].phoneNumber %>
                </th>
                <td>
                    <a href="/edit-crud?id=<%= dataTable[i].id %>" class="btn-edit" type="button">Edit</a>
                    <a href="/delete-crud?id=<%= dataTable[i].id %>" class="btn-delete" type="button">Delete</a>
                </td>
            </tr>
            <% } %>
    </table>
</body>

</html>